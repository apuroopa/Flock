@model dynamic
@{
    ViewBag.Title = "title";
    Layout = "../Shared/_Layout.cshtml";
}
<div class="container" ng-controller="messageController">
    <div class="jumbotron">
        <h3>Welcome, Let's flock together</h3>
    </div>
    <div ng-repeat="message in messages">
        <div class="row-fluid">
            <div class="span4">{{message.createDateTime}}</div>
        </div>
        <div class="row-fluid">
            <div class="span12">
                <blockquote>
                    <p>{{message.message}} </p>
                    <small>{{message.createUserId}}</small>
                </blockquote>
            </div>
        </div>
        <div class="row-fluid">
            <div class="span4">
                <i class="icon-bullhorn"></i> Shout out loud
            </div>
            <div class="span4">
                <i class="icon-glass"></i> Cheers, like it!
            </div>
        </div>
        <hr />
    </div>
</div>